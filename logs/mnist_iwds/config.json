{
    "training": {
        "local_epoch": 2,
        "global_rounds": 200,
        "batch_size": 64
    },
    "model": {
        "model_name": "mnist_cnn"
    },
    "optimizer": {
        "kind": "sgd",
        "learning_rate": 0.001
    },
    "federated_learning": {
        "num_clients": 10,
        "clients_ratio": 0.5
    },
    "dataset": {
        "name": "mnist",
        "long_tail_alpha": 0.9,
        "data_path": "./mnist_dataset"
    },
    "strategy": {
        "fed_iwds": {
            "beta": 0.99,
            "beta_zero": 0.9999,
            "rou": 0.992
        }
    },
    "system": {
        "device": "cuda",
        "seed": 42,
        "log_dir": "logs",
        "save_model": true,
        "save_path": "saved_models"
    }
}
